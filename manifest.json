{
  "name": "Facebook Contacts Sync",
  "description": "Syncs Facebook Contacts with Google Contacts",
  "version": "0.01",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": { "page": "background.html" },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://wwww.facebook.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": [ "js/jquery-3.1.0.min.js", "js/core.js", "js/in.js" ]
    }
  ],
  "browser_action": {
    //"default_icon": "images/icon.png",
    "default_title": "Faceboook Contacts Sync"
  },
  "icons": {
    //"128": "images/icon_128.png",
    //"16": "images/icon.png"
  }
}
